{
  "presets": [
    [
      "@babel/env",
      {
        "modules": "auto",
        "useBuiltIns": "usage",
        "corejs": "3.15.2",
        "debug": false
      }
    ],
    "react-app"
  ],
  "plugins": [
    ["@babel/plugin-proposal-private-methods", { "loose": true }]
  ],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    }
  }
}
